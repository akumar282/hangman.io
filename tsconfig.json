{
    "compilerOptions": {
      "target": "es2022",
      "module": "esnext",
      "moduleResolution": "node",
      "outDir": "dist",
      "strict": true,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }